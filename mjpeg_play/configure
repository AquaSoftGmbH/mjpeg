	echo "Compiling in MMX/SSE support for Pentium CPU"
;;
*) x86cpu="no"
   MTYPEFLAGS=""
   echo "Non x86 CPU: no MMX/SSE code will be included"
;;
esac




# Check whether --with-movtar-prefix or --without-movtar-prefix was given.
