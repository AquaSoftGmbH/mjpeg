    movtar_config_args="$movtar_config_args --exec-prefix=$movtar_config_exec_prefix"
    if test x${MOVTAR_CONFIG+set} != xset; then
	MOVTAR_CONFIG=$movtar_config_exec_prefix/bin/movtar-config
    fi
